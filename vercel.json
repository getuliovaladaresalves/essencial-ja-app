{
  "version": 2,
  "builds": [
    {
      "src": "api/auth/[...action].ts",
      "use": "@vercel/node"
    },
    {
      "src": "api/prestadores/[...params].ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/auth/(.*)",
      "dest": "/api/auth/[...action].ts"
    },
    {
      "src": "/api/prestadores/(.*)",
      "dest": "/api/prestadores/[...params].ts"
    }
  ],
  "functions": {
    "api/auth/[...action].ts": {
      "maxDuration": 30
    },
    "api/prestadores/[...params].ts": {
      "maxDuration": 30
    }
  }
}